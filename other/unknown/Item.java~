public class Item
{
	public static int priceClay;
	public static int priceLumber;
	public static int priceOre;
	public static int priceSheep;
	public static int priceWheat;
	
	public Item(int pClay, int pLumber, int pOre, int pSheep, int pWheat)
	{
		priceClay = pClay;
		priceLumber = pLumber;
		priceOre = pOre;
		priceSheep = pSheep;
		priceWheat = pWheat;
	}
}
